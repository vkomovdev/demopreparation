{
  "Log4NetCore": {
    "Name": "Main",
    "PropertyOverrides": [
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file",
        "Attributes": {
          "Value": "log/Exceptions.log"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/maximumFileSize",
        "Attributes": {
          "Value": "10MB"
        }
      },
      {
        "XPath": "/log4net/appender[@name='ConsoleAppender']/layout/conversionPattern",
        "Attributes": {
          "Value": "%date %-5level - %message%newline"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/layout/conversionPattern",
        "Attributes": {
          "Value": "%date %5level %logger.%method [%line]: %message%newline %exception"
        }
      },
      {
        "XPath": "/log4net/root/level",
        "Attributes": {
          "Value": "ERROR"
        }
      }
    ]
  },

  "ConnectionString": {
    "DefaultConnection": ""
  },

  "AllowedHosts": "",
  "BuildVersion": "",

  "CorsPolicyOptions": {
    "PolicyName": "",
    "Origins": ""
  },

  "Print": {
    "DefaultZplExtension": null,
    "DefaultPrinterKey": null,
    "Printers": [ ]
  }
}